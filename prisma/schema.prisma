generator client {
  provider = "prisma-client-js"
  previewFeatures = ["relationJoins"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model User{
  username String @id @db.VarChar(50)
  email String @db.VarChar(50) @unique 
  password String @db.VarChar(100) 
  image String? @db.VarChar(100)
  menus AddMenu[]
}
model AddMenu{
  addMenuId String @id @default(uuid())
  user User @relation(fields: [username], references: [username])  
  username String @db.VarChar(15)
  menu Menu @relation(fields: [code], references: [code])
  code Int
  createdAt DateTime @default(now()) 
  berat Float     
}
model Menu{
  code Int @id @default(autoincrement())
  nama String @db.VarChar(50)     
  air Float
  energi Float
  protein Float
  lemak Float
  kh Float
  serat Float
  abu Float
  kalsium Float
  fosfor Float
  besi Float
  natrium Float
  kalium Float
  tembaga Float
  seng Float
  retinol Float
  bKaroten Float
  karotenTotal Float
  thiamin Float
  riboflavin Float
  niasin Float    
  vitaminC Float
  bdd Float
  addMenus AddMenu[]
}
