generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User{
  username String @id @db.VarChar(15)
  email String @db.VarChar(50) @unique 
  password String @db.VarChar(100)   
  menus AddMenu[]
}
model AddMenu{
  user User @relation(fields: [username], references: [username])  
  username String @db.VarChar(15)
  menu Menu @relation(fields: [code], references: [code])
  code String @db.VarChar(5)
  createdAt DateTime @default(now()) 
  @@id([username, code])
}
model Menu{
  code String @id @db.VarChar(5)
  name String @db.VarChar(50)     
  air Float?
  energi Float?
  protein Float?
  lemak Float?
  kh Float?
  serat Float?
  abu Float?
  kalsium Float?
  fosfor Float?
  besi Float?
  natrium Float?
  kalium Float?
  tembaga Float?
  seng Float?
  vitC Float?
  bKar Float?
  karTot Float?
  thiamin Float?
  riboflavin Float?
  niasin Float?    
  addMenus AddMenu[]
}
